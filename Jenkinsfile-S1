pipeline {
    agent any
parameters{
	string(name:'pet',defaultValue :'Jimmy',description:'Name of my pet')
}
options{
retry(2)
}
    stages {
        stage('Hello') {
            steps {
                echo 'Environemnt s1'
		echo "my pet name is , ${params.jimmy}!"
		sh 'make'

            }
        }
    }
}
